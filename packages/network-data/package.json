{
	"name": "network-data",
	"packageManager": "yarn@3.6.0",
	"type": "module",
	"dependencies": {
		"express": "^4.18.2",
		"morgan": "^1.10.0",
		"swagger-ui-express": "^5.0.0",
		"tsoa": "^5.1.1"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/morgan": "^1.9.4",
		"@types/node": "^20.4.2",
		"@types/swagger-ui-express": "^4.1.3",
		"concurrently": "^8.2.0",
		"nodemon": "^3.0.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6"
	},
	"scripts": {
    	"predev": "npm run swagger",
    	"prebuild": "npm run swagger",
    	"swagger": "tsoa spec",
    	"build": "tsc",
		"start": "node build/index.js",
		"start:dev": "dev",
    	"dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\""
	},
	"nodemonConfig": {
		"watch": [
			"src"
		],
		"ext": "ts",
		"exec": "ts-node --esm src/index.ts"
	}
}
